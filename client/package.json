{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "local-web-server": "^3.0.7",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "rollup": "^1.10.1",
    "style-loader": "^1.0.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.13.6",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.prod.js --progress",
    "watch": "webpack-dev-server --config webpack.config.dev.js --progress",
    "serve-prod": "yarn inject-html && cd dist && ws -p 80",
    "inject-html": "perl -pi -e 's/%%API_URL%%/$ENV{API_URL}/g' dist/index.html"
  },
  "dependencies": {
    "svelte-routing": "^1.4.0"
  }
